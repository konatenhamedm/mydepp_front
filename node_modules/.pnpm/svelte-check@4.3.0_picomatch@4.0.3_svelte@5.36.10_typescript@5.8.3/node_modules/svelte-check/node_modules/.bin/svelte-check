#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/svelte-check@4.3.0_picomatch@4.0.3_svelte@5.36.10_typescript@5.8.3/node_modules/svelte-check/bin/node_modules:/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/svelte-check@4.3.0_picomatch@4.0.3_svelte@5.36.10_typescript@5.8.3/node_modules/svelte-check/node_modules:/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/svelte-check@4.3.0_picomatch@4.0.3_svelte@5.36.10_typescript@5.8.3/node_modules:/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/svelte-check@4.3.0_picomatch@4.0.3_svelte@5.36.10_typescript@5.8.3/node_modules/svelte-check/bin/node_modules:/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/svelte-check@4.3.0_picomatch@4.0.3_svelte@5.36.10_typescript@5.8.3/node_modules/svelte-check/node_modules:/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/svelte-check@4.3.0_picomatch@4.0.3_svelte@5.36.10_typescript@5.8.3/node_modules:/Volumes/konate/FREEWAN/FRONT-END/back_office_refonte/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
